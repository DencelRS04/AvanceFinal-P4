@page
@model WebProducto.Pages.LogInModel

@{
    ViewData["Title"] = "Iniciar Sesion";
}


<h1>Iniciar Sesion</h1>


<form method="post">
    <div class="form-group">
        <label asp-for="Usuario"></label>
        <input asp-for="Usuario" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Contraseña"></label>
        <input asp-for="Contraseña" class="form-control" />
    </div>

    <button type="submit" class="btn btn    -primary">Ingresar</button>
</form>

