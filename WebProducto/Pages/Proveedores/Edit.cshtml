@page
@model WebProducto.Pages.Proveedores.EditModel
@{
    ViewData["Title"] = "Editar Proveedor";
}

<h2>Editar Proveedor</h2>

<form method="post">

    <label>Cédula Jurídica</label>
    <input asp-for="Proveedor.CedulaJuridica" readonly />

    <label>Empresa</label>
    <input asp-for="Proveedor.Empresa" />

    <label>Contacto</label>
    <input asp-for="Proveedor.Contacto" />

    <label>Teléfono</label>
    <input asp-for="Proveedor.Telefono" />

    <label>Correo</label>
    <input asp-for="Proveedor.Correo" />

    <label>Estado</label>
    <input asp-for="Proveedor.Estado" readonly />

    <button class="btn btn-primary mt-3">Actualizar</button>
    <a asp-page="Index" class="btn btn-secondary">Cancelar</a>

</form>
